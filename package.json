{
  "name": "queueup",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "preweb": "mkdir -p public && cp docs/sw.js public/sw.js",
    "web": "expo start --web",
    "build:web": "mkdir -p public && cp docs/sw.js public/sw.js && expo export --platform web --output-dir docs",
    "deploy:apply": "wrangler d1 migrations apply DB",
    "deploy:push": "wrangler deploy --config api/wrangler.toml",
    "test": "vitest run --config api/vitest.config.ts",
    "test:watch": "vitest --config api/vitest.config.ts"
  },
  "dependencies": {
    "@block65/webcrypto-web-push": "^1.0.2",
    "@marsidev/react-turnstile": "^1.3.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.1.2",
    "@react-native-community/slider": "^4.5.2",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.27",
    "expo": "~54.0.12",
    "expo-camera": "~15.0.6",
    "expo-clipboard": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-media-library": "~17.0.1",
    "expo-status-bar": "~3.0.8",
    "lucide-react-native": "^0.546.0",
    "nativewind": "4.1.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "^13.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@cloudflare/vitest-pool-workers": "^0.9.11",
    "@cloudflare/workers-types": "^4.20251008.0",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-preset-expo": "~54.0.3",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.42.1"
  },
  "main": "expo/AppEntry",
  "private": true
}
